#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/University/MaternyCare/backend/node_modules/mime/node_modules:/mnt/f/University/MaternyCare/backend/node_modules:/mnt/f/University/MaternyCare/node_modules:/mnt/f/University/node_modules:/mnt/f/node_modules:/mnt/f/University/MaternyCare/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/University/MaternyCare/backend/node_modules/mime/node_modules:/mnt/f/University/MaternyCare/backend/node_modules:/mnt/f/University/MaternyCare/node_modules:/mnt/f/University/node_modules:/mnt/f/node_modules:/mnt/f/University/MaternyCare/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
