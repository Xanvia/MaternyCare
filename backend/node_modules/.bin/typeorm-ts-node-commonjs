#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/sda3/University/ThirdYearAcademics/Sem1/Project/MaternyCare/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.1/node_modules/typeorm/node_modules:/mnt/sda3/University/ThirdYearAcademics/Sem1/Project/MaternyCare/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.1/node_modules:/mnt/sda3/University/ThirdYearAcademics/Sem1/Project/MaternyCare/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/sda3/University/ThirdYearAcademics/Sem1/Project/MaternyCare/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.1/node_modules/typeorm/node_modules:/mnt/sda3/University/ThirdYearAcademics/Sem1/Project/MaternyCare/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.1/node_modules:/mnt/sda3/University/ThirdYearAcademics/Sem1/Project/MaternyCare/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
fi
